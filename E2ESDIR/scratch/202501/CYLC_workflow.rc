[runtime]
    [[root]]
        # Default settings for all tasks
        [[[job]]]
            batch system = slurm
        [[[environment]]]
            datetime = $CYLC_TASK_CYCLE_POINT
            config = $CYLC_SUITE_DEF_PATH/config.yml
            inputdata = $CYLC_SUITE_DEF_PATH/inputdata.yml
            INSTALL_PATH = $CYLC_SUITE_DEF_PATH
        [[[events]]]
            mail to = USEREMAIL
            mail events = failed
  
    [[lisda_run]]
        script = /discover/nobackup/projects/ghilis/S2S/GLOBAL/FileSharing/E2ESDIR/scratch/202501/lis_darun/lisda_run.sh
        [[[directives]]]
            --account=s1189
            --time=300
            --constraint=mil
            --ntasks=288 --ntasks-per-socket=48 --ntasks-per-core=1
            --job-name=lisda_
            --output /discover/nobackup/projects/ghilis/S2S/GLOBAL/FileSharing/E2ESDIR/scratch/202501/lis_darun/lisda_%j.out
