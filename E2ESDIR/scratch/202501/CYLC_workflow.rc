#!jinja2
# S2S Forecast Workflow
  
[meta]
    title = S2S Forecast Workflow
    description = S2S Forecast Initialized on YYYY-MM
  
[cylc]
    UTC mode = True
    cycle point format = %Y-%m-%dT%H
  
[scheduling]
    initial cycle point = YYYY-MM-01T00
    max active cycle points = 1
  
    [[dependencies]]
        [[[R1]]] # validity (hours)
            graph = """
                lisda_run => ldtics_run
                bcsd01_01_run & bcsd01_02_run & bcsd01_03_run & bcsd01_04_run & bcsd01_05_run & bcsd01_06_run & bcsd03_run => bcsd04_01_run
